{
    "builds": [
      {
        "src": "counterApp/wsgi.py",
        "use": "vercel/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "static/$1" },
      { "src": "/(.*)", "dest": "counterApp/wsgi.py" }
    ]
  }